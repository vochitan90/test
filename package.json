{
  "name": "EDocument",
  "version": "1.0.0",
  "license": "",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 8888 --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check",
    "start-4si": "ng serve --port 8100 --base-href /edocument/ --deploy-url /edocument/ --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check",
    "start-prod": "ng serve --port 8888 --base-href /edocument/ --deploy-url /edocument/ --proxy-config proxy.conf_prod.json --host 0.0.0.0 --disable-host-check",
    "start-hmr": "ng serve --configuration hmr --source-map=false --hmr-warning=false",
    "start-hmr-sourcemaps": "ng serve --configuration hmr --source-map=true --hmr-warning=false",
    "build": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev",
    "build-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --dev --stats-json",
    "build-prod": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod",
    "build-prod-2": "ng build --prod",
    "install:clean-mac": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm run start",
    "install:clean": "rimraf node_modules && rimraf package-lock.json && npm install && ng serve",
    "build-prod-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --prod --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "start-hrm": "ng serve --hmr",
    "postinstall": "ngcc"
  },
  "private": true,
